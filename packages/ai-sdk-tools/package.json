{
  "name": "ai-sdk-tools",
  "version": "0.9.0",
  "description": "Complete toolkit for building AI applications with the Vercel AI SDK - agents, state management, caching, artifacts, devtools, and memory",
  "keywords": [
    "ai",
    "ai-sdk",
    "agents",
    "multi-agent",
    "orchestration",
    "tools",
    "cache",
    "caching",
    "artifacts",
    "streaming",
    "state-management",
    "devtools",
    "memory",
    "llm",
    "openai",
    "anthropic",
    "google",
    "claude",
    "react",
    "zustand",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/midday-ai/ai-sdk-tools.git",
    "directory": "packages/ai-sdk-tools"
  },
  "bugs": {
    "url": "https://github.com/midday-ai/ai-sdk-tools/issues"
  },
  "homepage": "https://github.com/midday-ai/ai-sdk-tools#readme",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk-tools/agents": "file:../agents",
    "@ai-sdk-tools/artifacts": "file:../artifacts",
    "@ai-sdk-tools/cache": "file:../cache",
    "@ai-sdk-tools/devtools": "file:../devtools",
    "@ai-sdk-tools/memory": "file:../memory",
    "@ai-sdk-tools/store": "file:../store"
  },
  "peerDependencies": {
    "ai": ">=5.0.0",
    "@ai-sdk/react": ">=2.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=16.8.0",
    "zod": "^3.25.76 || ^4.1.8",
    "zustand": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/react": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "zustand": {
      "optional": true
    },
    "@upstash/redis": {
      "optional": true
    },
    "drizzle-orm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "zod": "^4.1.12",
    "ai": "^5.0.78"
  },
  "publishConfig": {
    "access": "public"
  }
}
