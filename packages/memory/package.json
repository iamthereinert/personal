{
  "name": "@ai-sdk-tools/memory",
  "version": "0.9.0",
  "description": "Persistent memory system for AI agents",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/providers/*.d.ts",
      "import": "./dist/providers/*.js",
      "require": "./dist/providers/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@ai-sdk-tools/debug": "file:../debug",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@upstash/redis": "^1.34.3",
    "drizzle-orm": "^0.36.0"
  },
  "peerDependenciesMeta": {
    "@upstash/redis": {
      "optional": true
    },
    "drizzle-orm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@upstash/redis": "^1.35.6",
    "drizzle-orm": "^0.44.7",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
